
sales_data = [[12, 17, 22], [2, 10, 3], [5, 12, 13]]

scoops_sold = 0
for location in sales_data:
  print(location)
  for i in location:
    scoops_sold += i #going through the numbers in each "location" element and adding them together
    # 12 + 7 + 22 + 2 + 10 + 3 + 5 + 12 + 13 = [96]
print(scoops_sold)

    
